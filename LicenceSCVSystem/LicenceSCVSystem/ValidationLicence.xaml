        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LicenceSCVSystem"
        mc:Ignorable="d"
        Title="ValidationLicence" Height="450" Width="800">
    <Grid>
        <Button x:Name="Validar" Content="Validar" HorizontalAlignment="Left" Margin="320,203,0,0" Click="Validar_Click" VerticalAlignment="Top" Width="75"/>
        <TextBlock x:Name="Licence" HorizontalAlignment="Left" Margin="343,92,0,0" TextWrapping="Wrap" Text="LicenceInfo" VerticalAlignment="Top"/>

    </Grid>
</Window>
